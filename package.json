{
	"name": "boilerplate",
	"version": "0.0.1",
	"description": "Tanishq Kancharla's website",
	"main": "src/index.ts",
	"scripts": {
		"clear": "mkdir -p dist && rm -rf dist/* && cp -r public/* dist",
		"build": "npm run clear && ts-node build.ts",
		"dev": "npm run clear && ts-node dev.ts",
		"serve": "serve dist",
		"deploy": "npx vercel deploy",
		"test": "mocha -r ts-node/register './src/**/*.test.ts' --verbose"
	},
	"author": "Tanishq Kancharla",
	"license": "MIT",
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/mocha": "^9.1.0",
		"@types/node-fetch": "^2.6.1",
		"@types/react": "^17.0.40",
		"@types/react-dom": "^17.0.13",
		"chokidar": "^3.5.3",
		"decache": "^4.6.1",
		"esbuild": "^0.13.15",
		"mocha": "^9.2.2",
		"serve": "^13.0.2",
		"ts-node": "^10.6.0",
		"typescript": "^4.6.2",
		"vercel": "^24.0.0"
	},
	"dependencies": {
		"estrella": "^1.4.1",
		"fs-extra": "^10.1.0",
		"motion": "^10.11.0",
		"node-fetch": "^2.6.7",
		"prosemirror-commands": "^1.3.0",
